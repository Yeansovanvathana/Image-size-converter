{
  "manifest_version": 3,
  "name":"Image Size Converter",
  "description": "convert image",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "img/icon22.png"
  },

  "icons": {
		"128": "img/icon128.png",
		"48": "img/icon48.png",
		"16": "img/icon16.png"
	},

  "host_permissions": [
    "<all_urls>"
  ],

  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
  }
}
